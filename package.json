    "css-loader": "^7.1.2",
    "jquery": "^3.7.1",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "tailwindcss": "^4.1.11",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
