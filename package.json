{
  "name": "@hubleto/project",
  "version": "0.14",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build-css": "npx @tailwindcss/cli -i ./theme.twcss -o ./assets/compiled/css/main.css",
    "watch-css": "npx @tailwindcss/cli --watch -i ./theme.twcss -o ./assets/compiled/css/main.css",
    "analyse-php": "vendor/bim/phpstan analyse",
    "build-js": "npx webpack --mode development",
    "watch-js": "npx webpack --watch --mode development",
    "build": "npx webpack --mode development && npx @tailwindcss/cli -m -i ./theme.twcss -o ./assets/compiled/css/main.css",
    "build:prod": "npx webpack --mode production && npx @tailwindcss/cli -m -i ./theme.twcss -o ./assets/compiled/css/main.css"
  },
  "dependencies": {
    "@babel/preset-react": "^7.27.1",
